<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.blueline.dao.ProductTypeDao">

	<select id="findAll" resultType="ProductType">
		SELECT * FROM we_product_type
	</select>
	
	<select id="findByType" resultType="ProductType">
		SELECT * FROM we_product_type WHERE product_type= #{productType}
	</select>

</mapper>
